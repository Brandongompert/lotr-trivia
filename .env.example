# Umami Analytics Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control

# PostgreSQL Database Configuration
UMAMI_DB_NAME=umami
UMAMI_DB_USER=umami
UMAMI_DB_PASSWORD=your_secure_password_here

# Umami App Secret (generate with: openssl rand -base64 32)
UMAMI_APP_SECRET=your_random_secret_here

# Frontend Environment Variables
# Get this from Umami dashboard after creating a website
VITE_UMAMI_WEBSITE_ID=your_website_id_here
VITE_UMAMI_URL=https://analytics.yourdomain.com

